<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="BookManager.Boundary.BookDisplayView">
   <children>
      <BorderPane prefHeight="522.0" prefWidth="800.0">
         <center>
            <TableView fx:id="browseBookTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <columns>
                  <TableColumn fx:id="bookidCol" prefWidth="75.0" text="Book ID" />
                  <TableColumn fx:id="titleCol" prefWidth="75.0" text="Title" />
                  <TableColumn fx:id="authorCol" prefWidth="75.0" text="Author" />
                  <TableColumn fx:id="publisherCol" prefWidth="75.0" text="Publisher" />
                  <TableColumn fx:id="categoryCol" prefWidth="75.0" text="Category" />
                  <TableColumn fx:id="isbnCol" prefWidth="75.0" text="ISBN" />
                  <TableColumn fx:id="discriptionCol" prefWidth="75.0" text="Discription" />
                  <TableColumn fx:id="priceCol" prefWidth="75.0" text="Price" />
               </columns>
            </TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
