<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserManager.Boundary.RegisterView">
  <center>
    <GridPane BorderPane.alignment="CENTER">
      <children>
        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
          <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="45.0" prefWidth="117.0" text="Username" textAlignment="CENTER" textOverrun="CLIP">
              <font>
                <Font name="System Bold" size="20.0" fx:id="x1" />
              </font>
            </Label>
            <TextField id="username" fx:id="usernameTF" prefHeight="25.0" prefWidth="140.0" />
            <Label font="$x1" prefHeight="40.0" prefWidth="115.0" text="Password" />
            <TextField id="password" fx:id="passwordTF" />
            <Label font="$x1" prefHeight="40.0" prefWidth="127.0" text="Full name" />
            <TextField id="fullname" fx:id="fullnameTF" prefHeight="25.0" prefWidth="140.0" />
            <Label font="$x1" prefHeight="40.0" prefWidth="127.0" text="Email" />
            <TextField id="email" fx:id="emailTF" prefHeight="25.0" prefWidth="140.0" />
            <Label font="$x1" prefHeight="40.0" prefWidth="127.0" text="Address" />
            <TextField id="address" fx:id="addressTF" prefHeight="25.0" prefWidth="140.0" />
            <HBox alignment="CENTER" prefHeight="53.0" prefWidth="267.0" spacing="8.0">
              <children>
                <Button id="signupButton" fx:id="registerButton" mnemonicParsing="false" onAction="#submitRegister" prefHeight="25.0" prefWidth="82.0" text="Register" />
              </children>
              <VBox.margin>
                <Insets />
              </VBox.margin>
            </HBox>
          </children>
        </VBox>
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="267.0" minWidth="10.0" prefWidth="215.0" />
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="567.0" minWidth="10.0" prefWidth="433.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="266.0" minWidth="10.0" prefWidth="152.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints maxHeight="193.0" minHeight="3.0" prefHeight="60.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="493.0" minHeight="10.0" prefHeight="493.0" valignment="CENTER" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="190.0" minHeight="10.0" prefHeight="47.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
  </center>
</BorderPane>
